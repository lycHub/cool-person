{
  "name": "@personal/workspace",
  "version": "0.0.1",
  "description": "Personal template",
  "type": "module",
  "private": "true",
  "scripts": {
    "start:icon": "pnpm -F=@personal/icons preview",
    "build:icon": "pnpm -F=@personal/icons build",
    "eslint:fix": "eslint --fix --max-warnings 0 --no-warn-ignored",
    "stylelint:fix": "stylelint packages/*/src/**/*.{scss,css,vue} --fix --max-warnings 0",
    "prepare": "husky",
    "commit": "cz && git push",
    "start": "pnpm -F=@personal/cool dev",
    "build": "pnpm -F=@personal/cool build",
    "preview": "pnpm -F=@personal/cool preview"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "keywords": [
    "personal",
    "vue"
  ],
  "author": "ysx",
  "workspaces": [
    "packages\\icons",
    "packages\\shared",
    "packages\\cool"
  ],
  "browserslist": [
    "defaults"
  ],
  "engines": {
    "node": ">=20.17.0",
    "npm": ">=10.8.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.0",
    "@commitlint/config-conventional": "^19.8.0",
    "@eslint/js": "^9.25.1",
    "@tsconfig/recommended": "^1.0.13",
    "@types/urijs": "^1.19.25",
    "@typescript-eslint/eslint-plugin": "^8.31.0",
    "@typescript-eslint/parser": "^8.31.0",
    "commitizen": "^4.3.1",
    "cz-conventional-changelog": "^3.3.0",
    "edgeone": "^1.2.29",
    "eslint-config-prettier": "^10.1.2",
    "eslint-import-resolver-typescript": "^4.3.4",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.6",
    "eslint-plugin-vue": "^10.5.1",
    "globals": "^16.0.0",
    "husky": "^9.1.7",
    "postcss": "^8.5.3",
    "postcss-html": "^1.8.0",
    "postcss-pxtorem": "^6.1.0",
    "postcss-scss": "^4.0.9",
    "prettier": "^3.5.3",
    "sass": "^1.87.0",
    "stylelint": "^16.19.1",
    "stylelint-config-recommended-vue": "^1.6.1",
    "stylelint-config-sass-guidelines": "^12.1.0",
    "stylelint-config-standard-scss": "^14.0.0",
    "stylelint-order": "^6.0.4",
    "stylelint-scss": "^6.11.1",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.31.0"
  },
  "dependencies": {
    "@faker-js/faker": "^9.7.0",
    "axios": "^1.9.0",
    "clsx": "^2.1.1",
    "consola": "^3.4.2",
    "dayjs": "^1.11.13",
    "decimal.js": "^10.6.0",
    "destr": "^2.0.5",
    "es-toolkit": "^1.43.0",
    "sa-sdk-javascript": "^1.27.11",
    "unstorage": "^1.15.0",
    "urijs": "^1.19.11",
    "zod": "^3.25.67"
  },
  "packageManager": "pnpm@9.15.4+sha512.b2dc20e2fc72b3e18848459b37359a32064663e5627a51e4c74b2c29dd8e8e0491483c3abb40789cfd578bf362fb6ba8261b05f0387d76792ed6e23ea3b1b6a0"
}
